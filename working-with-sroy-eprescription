INSERT INTO `prescription` (`PRESCRIPTION_ID`, `VISIT_ID`, `REFERRED_TO`, `DIET`, `NEXT_VISIT`, `ANY_OTHER_DETAILS`, `STATUS`, `chamber_id`, `created_by_user_id`, `create_date`, `isSync`, `doc_id`) 
