INSERT INTO `visit` (`VISIT_ID`, `PATIENT_ID`, `VISIT_DATE`, `APPOINTMENT_TO_DOC_NAME`, `VISITED`, `chamber_id`, `created_by_user_id`, `create_date`, `isSync`, `doc_id`) 
